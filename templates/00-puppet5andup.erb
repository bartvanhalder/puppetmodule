#This file is managed by Puppet, do not change manually!

<% if not @release_version.to_s.strip.empty? -%> 
Package: puppet<%= @major_version %>-release
Pin: version <%= @release_version %><%= @lsbdistcodename %>
Pin-Priority: 501
<% end %>

<% if not @agent_version.to_s.strip.empty? -%> 
Package: puppet-agent
Pin: version <%= @agent_version %><%= @lsbdistcodename %>
Pin-Priority: 501
<% end %>

<% if not @server_version.to_s.strip.empty? -%> 
Package: puppetserver
Pin: version <%= @server_version %><%= @lsbdistcodename %>
Pin-Priority: 501
<% end %>

<% if not @db_version.to_s.strip.empty? -%> 
Package: puppetdb
Pin: version <%= @db_version %><%= @lsbdistcodename %>
Pin-Priority: 501
<% end %>
